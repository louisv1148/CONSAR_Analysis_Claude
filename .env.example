# CONSAR Monitor Environment Configuration
# Copy this file to .env and fill in your email settings

# Email settings for notifications (Gmail recommended)
CONSAR_EMAIL_USER=your-email@gmail.com
CONSAR_EMAIL_PASSWORD=your-app-password
CONSAR_NOTIFY_EMAIL=your-email@gmail.com

# Optional: Customize monitoring settings
CONSAR_CHECK_INTERVAL_HOURS=24
CONSAR_RETENTION_DAYS=30